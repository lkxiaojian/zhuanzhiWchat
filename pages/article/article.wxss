.toolbar {
  margin-bottom: 0rpx;
}

.article_top {
  position: relative;
  width: 92%;
  margin: 0 auto;
  background: white;
  height: 430rpx;
  border: 1px solid white;
}

.article_content {
  background: rgb(242, 242, 242);
  padding-top: 110rpx;
}

.pdfStyle {
  width: 28rpx;
  height: 28rpx;
  margin-left: 12rpx;
}

.photo {
  position: absolute;
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  left: 50%;
  margin-left: -80rpx;
  margin-top: -80rpx;
}

.article_text {
  margin-top: 130rpx;
  text-align: center;
  color: rbg(51, 51, 51);
  font-weight: bold;
}

.at_bot {
  width: 93%;
  margin: 0 auto;
  margin-top: 50rpx;
  display: flex;
  justify-content: space-around;
}

.at_bot_left, .at_bot_right {
  display: flex;
  width: 170rpx;
  background: rgb(249, 248, 253);
  border-radius: 18rpx;
  text-align: center;
  padding: 36rpx 50rpx;
}

.left_circle, .right_circle {
  display: inline-block;
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  margin-top: -4rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}

.left_circle {
  background-color: rgb(108, 90, 252);
}

.bt_left {
  margin: 0 auto;
}

.bt_right {
  margin-left: 20rpx;
}

.right_circle {
  background-color: rgb(252, 187, 66);
}

.left_text {
  color: rgb(153, 153, 153);
  font-size: 22rpx;
}

.left-num {
  width: 20rpx;
  color: rgb(34, 34, 34);
  font-size: 36rpx;
  margin-top: 18rpx;
  margin-left: 20rpx;
}

.left-nums {
  color: rgb(34, 34, 34);
  font-size: 36rpx;
  margin-top: 18rpx;
  margin-left: -36rpx;
}
.lists {
  display: flex;
  flex-direction: column;
  overflow-y: scroll;
  width: 97%;
  margin: 0 auto;
  /* margin-top: 26rpx; */
  margin-bottom: 28rpx;
}
.cart_content {
  margin-top: 30rpx;
  margin-bottom: 22rpx;
}

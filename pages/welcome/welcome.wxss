/* pages/welcome/welcome.wxss */

.container {
  /* border:1px solid red; *//* width:90%; *//* margin:0 auto; */
  background-size: 100% 90%;
  overflow: hidden;
  background-repeat: no-repeat;
  background-position-y: -150%;
}

.status_toolbar {
  background-color: #ff8b2a;
}

.moddle {
  width: 100%;
  color: white;
  flex: 1;
  display: inline-block;
  flex-direction: column;
}

.top_line {
  padding-top: 120rpx;
  display: flex;
  align-items: flex-end;
  height: 49rpx;
  width: 100%;
  color: white;
}

.top_title {
  flex: 1;
  margin-left: 16rpx;
  font-size: 36rpx;
  color: rgb(42, 41, 41);
}

.wel_line {
  width: 6rpx;
  height: 24rpx;
  border-radius: 4rpx;
  float: left;
  background: rgb(220, 149, 62);
  margin-top: 10rpx;
  margin-right: 10rpx;
}

.wel_title {
  margin-left: 10rpx;
  font-size: 30rpx;
  color: rgb(42, 41, 41);
}

.dialog_style {
  position: absolute;
  background: rgb(172, 172, 172);
  padding: 8rpx 50rpx;
  font-size: 22rpx;
  color: white;
  border-radius: 6rpx;
  margin-top: 62%;
}

button {
  outline: 0 none !important;
}

.top_menu {
  width: 80rpx;
  height: 40rpx;
  font-size: 24rpx;
  margin-right: 22rpx;
  background-color: rgba(0, 0, 0, 0.18);
  color: white;
  line-height: 40rpx;
  text-align: center;
  border-radius: 6rpx;
}

.free-btn-bordernone::after {
  border: none;
}

.wel_bt {
  width: 95%;
  margin: 0 auto;
  margin-top: 52rpx;
}

.title {
  height: 46rpx;
  border-radius: 6rpx;
  display: inline-block;
  margin: 26rpx 24rpx 0 14rpx;
  padding: 0 28rpx 0 28rpx;
  line-height: 46rpx;
  font-size: 24rpx;
}

.title1, .title2, .title3, .title4, .title5, .title6, .title7, .title8, .title9,
.title10, .title11, .title12, .title13, .title14, .title15 {
  background: white;
  color: rgb(220, 149, 62);
  border: 1px solid rgb(220, 149, 62);
}

.title_on1, .title_on2, .title_on3, .title_on4, .title_on5, .title_on6,
.title_on7, .title_on8, .title_on9, .title_on10, .title_on11, .title_on12,
.title_on13, .title_on14, .title_on15 {
  background: rgb(220, 149, 62);
  color: white;
  border: 1px solid rgb(220, 149, 62);
}

.bottom_text {
  color: rgb(45, 45, 45);
  margin: 0 auto;
  margin-top: 88rpx;
  font-size: 40rpx;
  width: 100%;
  text-align: center;
}

.startApp {
  width: 484rpx;
  margin-top: 66rpx;
  background-color: white;
  border-radius: 50rpx;
  background: rgb(220, 149, 62);
  border: solid rgb(220, 149, 62) 2rpx;
  font-size: 32rpx;
  color: rgb(255, 255, 255);
}
